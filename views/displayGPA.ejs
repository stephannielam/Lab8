<div>
    <% if(rs.length > 0) { %>
    <table border="1">
        <tr><th>Company</th>
        <th>Average GPA</th></tr>
        <% for (var i=0; rs.length > i; i++) { %>
        <tr>
            <td><a> <%= rs[i].Company %></a></td>
            <td><a><%= rs[i].Average_GPA %></a></td>
        </tr>
        <% } %>
    </table>


    <% } else { %>
    No accounts are entered into the Account database table.
    <% } %>
</div>

