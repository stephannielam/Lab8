<div>
    <% if(rs.length > 0) { %>
    <table border="1">
        <tr><th>Name</th>
            <th>Company Name</th>
            <th>Job Description</th></tr>
        <% for (var i=0; rs.length > i; i++) { %>
        <tr>
            <td><a> <%= rs[i].name %></a></td>
            <td><a><%= rs[i].Company_Name %></a></td>
            <td><a><%= rs[i].Description %></a></td>
        </tr>
        <% } %>
    </table>


    <% } else { %>
    No accounts are entered into the Account database table.
    <% } %>
</div>

